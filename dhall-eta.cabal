    type: exitcode-stdio-1.0
    main-is: Dhall/Eta/Test/Main.hs
    build-depends:
        base >=4.5 && <5,
        dhall >=1.19.1 && <1.20,
        dhall-eta -any,
        directory >=1.2.2.0 && <1.4,
