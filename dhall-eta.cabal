source-repository this
    type: git
    location: https://github.com/eta-lang/dhall-eta
    tag: 1.0.0

library
    exposed-modules:
        Dhall.Eta
        Dhall.Eta.Binary
        Dhall.Eta.Context
        Dhall.Eta.Core
        Dhall.Eta.Core.Java
        Dhall.Eta.Import
        Dhall.Eta.Parser
        Dhall.Eta.Parser.Java
        Dhall.Eta.TypeCheck
        Dhall.Eta.TypeCheck.Java
        Eta.Types
    build-depends:
        base >=4.5 && <5,
        bytestring >=0.10 && <0.11,
        containers >=0.5 && <0.6,
        contravariant >=1.5 && <1.6,
        cryptonite >=0.23 && <1.0,
        dhall >=1.19.1 && <1.20,
        dotgen >=0.4.2 && <0.5,
        eta-java-interop >=0.1.5.0 && <0.1.6,
        megaparsec >=6.1.1 && <7.1,
        memory >=0.14 && <0.15,
        lens-family-core >=1.0.0 && <1.3,
        prettyprinter >=1.2.0.1 && <1.3,
        scientific >=0.3.0.0 && <0.4,
        serialise >=0.2.0.0 && <0.3,
        text >=1.2 && <1.3,
        transformers >=0.2.0.0 && <0.6
    java-sources:
        @classes.java
    default-language: Haskell2010
    default-extensions: DataKinds TypeOperators TypeFamilies
                        FlexibleContexts MultiParamTypeClasses OverloadedStrings
    hs-source-dirs: src/main/eta
    other-modules:
        Dhall.Eta.Map
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

executable  dhall-eta-all
    main-is: Main.hs
    build-depends:
        base >=4.5 && <5,
        dhall-eta -any
    default-language: Haskell2010
    default-extensions: DataKinds TypeOperators TypeFamilies
                        FlexibleContexts MultiParamTypeClasses OverloadedStrings
    hs-source-dirs: examples/src/main/eta
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

